
<%= form_for @boat do |f| %>
	<div>Boat Name <%= f.text_field :boat_name %></div>
	<div>Container Capacity: <%= f.text_field :container_capacity %></div>
	<div>Port Of Origin: <%= f.text_field :port_of_origin %></div>
	<%= f.hidden_field :user_id, :value => current_user.id %>
	<%= f.submit %>
<% end %>
<%= link_to "Back", :back %>
